{
  "compilerOptions": {
    "strict": true,
    "noEmit": true,
    "module": "ES2020",
    "moduleResolution": "Node",
    "target": "ES2020",
    "types": ["vite/client", "node"],
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "useDefineForClassFields": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
    }
  }
}
